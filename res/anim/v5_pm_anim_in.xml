<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:fillAfter="true"
    android:fillBefore="true" 
    android:zAdjustment="bottom"
    >
    <translate 
        android:fromYDelta="100"
        android:toYDelta="@integer/v5_widget_anim_pmtoy_up"
        android:interpolator="@android:anim/accelerate_interpolator"
        android:duration="200"
        android:startOffset="350"
        />
    <alpha android:fromAlpha="0.0" android:toAlpha="1.0" android:duration="300" android:startOffset="350"/>
     <translate 
        android:fromYDelta="@integer/v5_widget_anim_pmtoy_up"
        android:toYDelta="@integer/v5_widget_anim_pmtoy_down"
        android:interpolator="@android:anim/decelerate_interpolator"
        android:duration="80"
        android:startOffset="600"
        />
</set>